{
  "name": "jira-url-wizard",
  "displayName": "JIRA URL Wizard",
  "version": "1.0.2",
  "description": "A browser extension for managing JIRA environment URLs",
  "author": "Khalil Charfi",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "postbuild": "node scripts/optimize.js",
    "package": "plasmo package",
    "install-image-tools": "./scripts/install-image-tools.sh",
    "optimize-images": "./scripts/optimize-images.sh ./build/chrome-mv3-prod"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@plasmohq/messaging": "^0.7.1",
    "@plasmohq/storage": "^1.15.0",
    "@types/dompurify": "^3.2.0",
    "@types/marked": "^6.0.0",
    "dompurify": "^3.2.5",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "lucide-react": "^0.487.0",
    "marked": "^15.0.8",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^15.4.1",
    "react-qrcode-logo": "^3.0.0",
    "react-tag-input": "^6.10.6"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.19",
    "clean-css-cli": "^5.6.3",
    "glob": "^11.0.1",
    "html-minifier-terser": "^7.2.0",
    "postcss": "^8.4.38",
    "prettier": "3.2.4",
    "tailwindcss": "^3",
    "terser": "^5.39.0",
    "typescript": "5.3.3"
  },
  "manifest": {
    "name": "__MSG_extensionName__",
    "default_locale": "en",
    "permissions": [
      "tabs",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*.webp"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}
